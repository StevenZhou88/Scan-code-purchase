.carousel{
  width:100%;
  height:550rpx;
  border-radius:100% 100% 100% 100% / 0% 0% 20% 20%;
  overflow:hidden;
  padding:0;
  margin:0;
  position: relative;
  border-bottom: none;
  box-shadow:0px 0px 11px gainsboro;
}
.slide-image{
  width: 100%;
  height: 100%;
  padding:0;
  margin:0;
  
}
.position{
  width: 100%;
  height: 80rpx;
  background-color: white;
  position: absolute;
  top: 0;
  opacity:.6;
  z-index: 9;
  display: flex;
  justify-content: center;
  align-items: center;
}
.img_position{
  width: 26px;
  height: 22px;
  margin-right: 10rpx;
}
.qccode{
  width: 95%;
  margin-left: 10px;
  height: 45%;
  position: absolute;
  background: white;
  bottom: 0px;
  border-top-left-radius: 52rpx;
  border-top-right-radius: 52rpx;
  display: flex;
  overflow: hidden;
}
.nav{
  width: 100%;
  height: 150rpx;
  margin-top: 50rpx;
  display: flex;
  justify-content: space-around;
}
.nav image{
  width: 100%;
  height: 100%;
}
.banna{
  width: 95%;
  margin: 0 auto;
  height: 165rpx;
   margin-top: 60rpx;
}
.banna image{
  width: 100%;
  height: 100%;
   border-radius: 10px;
}
